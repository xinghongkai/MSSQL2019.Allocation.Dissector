<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
</startup>
	
  <connectionStrings>	  
	<add name="VdiConnection" connectionString="Driver={SQL Server};Server=(local);Trusted_Connection=Yes;" />  
  </connectionStrings>
  <appSettings>
    <add key="BackUpSql" value="BACKUP DATABASE NISStatistic TO VIRTUAL_DEVICE='{0}' WITH STATS = 1" />
  </appSettings>
</configuration>
